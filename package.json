{
  "name": "nestjs-skeleton",
  "version": "0.0.1",
  "description": "NejstJS API Server Skeleton",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": ">=v18.12.0",
    "npm": ">=8.19.2"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "DEBUG=app*,error*,debug* node ./node_modules/pm2/bin/pm2 start process.json && npm run poststart",
    "format": "prettier --write \"src/**/*.ts\"",
    "start:local": "NODE_ENV=local nest start --watch",
    "start:localdebug": "NODE_ENV=local nest start --debug --watch",
    "start:dev": "NODE_ENV=development nest start --watch",
    "start:qa": "NODE_ENV=qa nest start --watch",
    "start:debug": "NODE_ENV=development nest start --debug --watch",
    "start:prod": "NODE_ENV=prod nest start --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "poststart": "node ./node_modules/pm2/bin/pm2 logs --raw",
    "test:local": "NODE_ENV=test jest --runInBand --detectOpenHandles --forceExit --reporters=default",
    "test": "NODE_ENV=test jest --ci --runInBand --detectOpenHandles --forceExit --reporters=default --reporters=jest-junit --coverage"
  },
  "dependencies": {
    "@nestjs/axios": "^2.0.0",
    "@nestjs/cache-manager": "^2.1.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.0.0",
    "@nestjs/event-emitter": "^2.0.4",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/microservices": "^9.4.3",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/swagger": "^6.3.0",
    "@nestjs/typeorm": "^10.0.2",
    "@scalar/nestjs-api-reference": "^0.3.76",
    "axios": "^1.4.0",
    "bcrypt": "^5.0.1",
    "cache-manager": "^5.2.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cross-fetch": "^4.0.0",
    "js-base64": "^3.7.5",
    "lodash": "^4.17.21",
    "morgan-body": "^2.6.9",
    "pm2": "^5.3.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "ts-morph": "^19.0.0",
    "typeorm": "^0.3.20",
    "typeorm-naming-strategies": "^4.1.0",
    "typeorm-transactional": "^0.5.0",
    "typescript-retry-decorator": "^2.4.2",
    "uuid": "^9.0.0",
    "webpack": "^5.89.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cache-manager": "^4.0.3",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.8",
    "@types/lodash": "^4.14.202",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.0.0",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.7.4"
  }
}
